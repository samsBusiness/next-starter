{
  "extends": "next/core-web-vitals",
  "rules":{
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
    "indent": ["error", 2],
    "import/no-unused-modules": "error",
    "react/jsx-sort-props": ["error", { "ignoreCase": true }],
    "no-unused-vars": "error",
    "no-console": "warn",
    "consistent-return": "error"
  }
}
